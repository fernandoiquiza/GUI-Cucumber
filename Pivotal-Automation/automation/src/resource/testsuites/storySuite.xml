            <class name="org.fundacion.automation.story.CreateDescriptionTest"/>
            <class name="org.fundacion.automation.story.UpdateStoryEstimateTest"/>
            <class name="org.fundacion.automation.story.DeleteStoryTest"/>
            <class name="org.fundacion.automation.story.CreateCommentTest"/>
        </classes>
    </test>
