<?xml version="1.0" encoding="UTF-8" ?>
<suite name="WorkspaceSuite">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="org.fundacion.common.listener.ExecutionListener" />
    </listeners>
    <test name="WorkspaceTest">
        <classes>
            <class name="org.fundacion.automation.workspaces.TestCreateWorkspace"/>
            <class name="org.fundacion.automation.workspaces.TestDeleteWorkspace"/>
            <class name="org.fundacion.automation.workspaces.AddWorkspaceProjects"/>
            <class name="org.fundacion.automation.workspaces.RemoveWorkspaceProjects"/>
            <class name="org.fundacion.automation.workspaces.DeleteWorkspaceWithProject"/>
        </classes>
    </test>
</suite>